ce":"components/Root/Springboard/OpenedFolderBackdrop/OpenedFolderBackdrop.js","original":{"line":38,"column":1}},{"generated":{"line":58,"column":1},"source":"components/Root/Springboard/OpenedFolderBackdrop/OpenedFolderBackdrop.js","original":{"line":24,"column":0}},{"name":"OpenedFolderBackdrop","generated":{"line":60,"column":15},"source":"components/Root/Springboard/OpenedFolderBackdrop/OpenedFolderBackdrop.js","original":{"line":40,"column":15}}],"sources":{"components/Root/Springboard/OpenedFolderBackdrop/OpenedFolderBackdrop.js":"import React from 'react'\nimport styled from 'styled-components'\nimport { animated, useSpring } from 'react-spring'\n\nconst Wrapper = styled(animated.div)`\n    backdrop-filter: blur(20px);\n    height: 100%;\n    width: 100%;\n    left: 0;\n    top: 0;\n    position: absolute;\n\n    :hover {\n        cursor: pointer;\n    }\n`\n// need these because need to create the capability\n// of when to capture pointer events\n// instead of always 'pointer-events: none' in the CSS\nconst MAXIMUM_BLUR = 20\nconst MINIMUM_OPEN_AMOUNT_ON_SHOW = 0.1 // min to enable pointer events\nconst MINIMUM_OPEN_AMOUNT_ON_HIDE = 0.9 // when to disable\n\nconst OpenedFolderBackdrop = ({ isVisible, onClose: pushClose }) => {\n    const spring = useSpring({ openAmount: isVisible ? 1 : 0 })\n    const style = {\n        backdropFilter: spring.openAmount.interpolate(openAmount => `blur(${openAmount * MAXIMUM_BLUR}px)`),\n        opacity: spring.openAmount,\n        pointerEvents: spring.openAmount.interpolate(openAmount => {\n            return (isVisible && \n                openAmount >= MINIMUM_OPEN_AMOUNT_ON_SHOW) || \n                (!isVisible && openAmount >= MINIMUM_OPEN_AMOUNT_ON_HIDE) \n                ? 'auto' : 'none'\n        })\n    }\n\n    return <Wrapper onClick={() => pushClose()} style={style}/>\n}\n\nexport default OpenedFolderBackdrop;"},"lineCount":null}},"error":null,"hash":"9049d3d21843ef1f436a4625cdd0d33b","cacheData":{"env":{}}}